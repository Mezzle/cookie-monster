{
  "manifest_version": 2,
  "name": "Cookie Monster",
  "description": "Clears cookies for current website",
  "version": "0.1",
  "browser_action": {
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "scripts/jquery.min.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "cookies",
    "tabs",
    "<all_urls>"
  ]
}
